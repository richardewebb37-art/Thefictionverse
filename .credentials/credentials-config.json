{
  "build": {
    "production": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "project": {
    "name": "The Fictionverse",
    "slug": "the-fictionverse-",
    "owner": "mortalash2025",
    "projectId": "21f983d2-2452-4cbe-8472-ebdcc8962bbc",
    "version": "1.2.0",
    "runtimeVersion": "1.2.0"
  },
  "rules": {
    "doNotRegenerate": ["token", "keystore", "credentials"],
    "doNotMove": [".credentials/", "android/credentials/", ".env", "credentials.json"],
    "doNotDelete": ["keystore", "token", "credentials", ".env.local"],
    "alwaysUse": [".credentials/.env", "android/credentials/fictionverse-keystore.jks"]
  }
}